<record id="arabic_withgrade_model" model="ir.sequence">
    <field name="name">Arabic Withgrade Sequence</field>
    <field name="code">arabic.withgrade</field>
    <field name="prefix">G</field>
    <field name="padding">5</field>
    <field name="number_next_actual">1</field>
    <field name="number_increment">1</field>
    <field name="number_last">1</field>
</record>

<record id="arabic_nograde_model" model="ir.sequence">
    <field name="name">Arabic Nograde Sequence</field>
    <field name="code">arabic.nograde</field>
    <field name="prefix">NG</field>
    <field name="padding">5</field>
    <field name="number_next_actual">1</field>
    <field name="number_increment">1</field>
    <field name="number_last">1</field>
</record>


    <template id="assets_backend" name="almaaqal_grade assets" inherit_id="web.assets_backend">
        <xpath expr="." position="inside">
            <link rel="stylesheet" type="text/css" href="/almaaqal_grade/static/src/css/custom_styles.css"/>
