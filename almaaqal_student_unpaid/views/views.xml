<record id="view_customer_profit_wizard_wizard_data" model="ir.ui.view">
       <field name="name">due.student.wizard.view</field>
       <field name="model">due.student.wizard</field>
       <field name="type">form</field>
       <field name="arch" type="xml">
           <form>
            <label for="requested_costumer" name="requested_costumer"  invisible="1"/>
               <field name="requested_costumer"  widget="many2many_tags"   invisible="1"/>
            <label for="date_start" name="date_start"   invisible="1"/>
               <field name="date_start"   invisible="1"/>
            <label for="date_end" name="date_end"   invisible="1"/>   
                <field name="date_end"   invisible="1" />
               <footer>
                   <button name="action_done" string="Print" type="object" class="oe_highlight"/>
                   <button string="Cancel" class="btn btn-default" special="cancel"/>
               </footer>
           </form>
       </field>
    </record>



            <record id="action_customer_profit_wizard_wiz" model="ir.actions.act_window">
                <field name="name">Unpaid Student Report</field>
                <field name="type">ir.actions.act_window</field>
                <field name="res_model">due.student.wizard</field>
                <field name="view_mode">form</field>
                <field name="context">{}</field>
                <field name="view_id" ref="almaaqal_student_unpaid.view_customer_profit_wizard_wizard_data"/>
                <field name="target">new</field>
            </record>

            <menuitem action="action_customer_profit_wizard_wiz" id="manu_customer_profit_wizard_wiz" parent="almaaqal_hall.almaaqal_hall_menu_root" sequence="15" name="Unpaid Student Report"/>


