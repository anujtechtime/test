<record id="sale_orderform_view_oppor_revenue_user_id_data" model="ir.ui.view">
            <field name="name">sale.order.data</field>
            <field name="model">sale.order</field>
            <field name="inherit_id" ref="sale.view_order_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='partner_id']" position="after">
                    <!-- <field name="sponsor"/> -->
                    <field name="college_number"/>
                    <field name="student_no"/>
                    <field name="year" options="{'no_create_edit' : True}"/>
                    <field name="college" options="{'no_create_edit' : True}"/>
                    <field name="department" domain="[('college', '=', college)]" context="{'default_college': college}" options="{'no_create_edit' : True}"/>

                    <field name="student" options="{'no_create_edit' : True}"/>
                    <field name="Subject"/>
                    <field name="level"/>
                    
<!--                     <field name="installment_no" widget="monetary"/>
 -->                    
                </xpath>

                <xpath expr="//field[@name='date_order']" position="after">
                    <field name="amount"/>
                </xpath>
                
              </field>
            </record>


            <record id="sale_orderform_view_paymet_data" model="ir.ui.view">
            <field name="name">account.payment.data</field>
            <field name="model">account.payment</field>
            <field name="inherit_id" ref="account.view_account_payment_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='journal_id']" position="after">
                    <field name="level"/>
                    <field name="college" options="{'no_create_edit' : True}"/>
                    <field name="student" options="{'no_create_edit' : True}"/>
                    <field name="department" options="{'no_create_edit' : True}"/>
                    <field name="Subject"/>
                </xpath>
              </field>
            </record>


            <record id="sale_orderform_view_account_move" model="ir.ui.view">
            <field name="name">account.move.data</field>
            <field name="model">account.move</field>
            <field name="inherit_id" ref="account.view_move_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='partner_id']" position="after">
                    <field name="level" options="{'no_create_edit' : True}"/>
                    <field name="college" options="{'no_create_edit' : True}"/>
                    <field name="student" options="{'no_create_edit' : True}"/>
                    <field name="Subject" options="{'no_create_edit' : True}"/>
                    <field name="payment_id"/>
                </xpath>
              </field>
            </record>

  </data>
</odoo>