        <record model="ir.ui.view" id="registration_status_tree">
          <field name="name">Registration Status</field>
          <field name="model">registration.status</field>
          <field name="arch" type="xml">
            <tree>
                <field name="name"/>
            </tree>
          </field>
        </record>


        <record model="ir.actions.act_window" id="registration_status_tree_action_windownew_work">
          <field name="name">Registration Status </field>
          <field name="res_model">registration.status</field>
          <field name="view_mode">tree,form</field>
        </record>

        <record id="ir_actions_server_res_registration_status" model="ir.actions.server">
        <field name="name">Registartion Status</field>
        <field name="type">ir.actions.server</field>
        <field name="model_id" ref="model_res_partner"/>
        <field name="state">code</field>
        <field name="code">
            if records:
                action = records.assign_registrtion_status()
        </field>
        <field name="binding_model_id" ref="base.model_res_partner"/>
        <field name="binding_view_types">list,form</field>
    </record>


    <record id="view_sale_order_inherit_search_payment" model="ir.ui.view">
      <field name="name">subject.subject.tree</field>
      <field name="model">subject.subject</field>
      <field name="inherit_id" ref="almaaqal_grade.subject_subject_tree"/>
      <field name="arch" type="xml">
        <xpath expr="//field[@name='stage']" position="after">
            <field string="status_type"/>
            <field string="status_in_year"/>
            <field string="semrating2"/>
            <field string="rating_in_year"/>
            <field string="level"/>
            <field string="note"/>
            
            
          <!-- This field will show up on the default filters
               &lt; is the XML encoding for < and &gt; is the XML encoding for > -->
<!--           <filter string="Total &lt; 1000" name="total_under_1000" domain="[('amount_total', '&lt;', 1000)]"/>
          <filter string="Total &gt;= 1000" name="total_above_1000" domain="[('amount_total', '&gt;=', 1000)]"/> -->
        </xpath>
      </field>
    </record>

